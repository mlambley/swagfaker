{
    "name": "mlambley/swagfaker",
    "description": "Create fake data from a Swagger schema",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Lambley",
            "email": "mlambley@gmail.com"
        }
    ],
    "require": {
        "fzaninotto/faker": "^1.7"
    },
    "require-dev": {
        "codeception/aspect-mock": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "mlambley/swagception": "0.0.1-alpha.6"
    },
    "autoload": {
        "psr-4": {
            "SwaggerFaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwaggerFaker\\Test\\": "test/"
        }
    },
    "scripts": {
        "test": [
            "phpunit test/"
        ],
        "lint": [
            "php ./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix ."
        ]
    }
}
